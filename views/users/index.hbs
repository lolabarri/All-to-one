<div class="info">
    <h3>List of all users</h3>
    <ul>
        {{#each users}}
        <li>
            <a href="/users/{{this._id}}">{{this.name}}</a>
            |
            <a href="/users/{{this._id}}/delete">Delete</a>
        </li>
        {{/each }}
    </ul>
</div>

<script src="https://unpkg.com/axios/dist/axios.min.js"></script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAzm6wO-fMPB7-afq0yzoT8jNiboqGaIm0"></script>